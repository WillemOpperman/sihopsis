= simple_form_for(@associate_financial_document_line, :html => { :class => 'form-horizontal' }) do |f|
  = f.error_messages

  .form-inputs
    = f.input :uid
    = f.input :code
    = f.association :associate_financial_document
    = f.association :client_financial_document_line
    = f.association :campaign_schedule_placement
    = f.association :client_commission_deal_associate_member
    = f.association :client_billing_setting_line
    = f.association :media_category
    = f.input :amount
    = f.input :amount_home
    = f.input :tax_amount
    = f.input :amount_inc_tax_amount
    = f.input :amount_inc_tax_home_amount
    = f.input :eis_sync_date

  .form-actions
    = f.button :wrapped,
               :delete => @associate_financial_document_line,
               :cancel => associate_financial_document_lines_path,
               :data => { :disable_with => t(:saving, :scope => :forms) }
