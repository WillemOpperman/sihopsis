= simple_form_for(@service_provider, :html => { :class => 'form-horizontal' }) do |f|
  = f.error_messages

  .form-inputs
    = f.input :code
    = f.input :uid
    = f.input :name
    = f.input :is_this_agency
    = f.input :default_proof_file_extention
    = f.input :is_active
    = f.input :modified_date
    = f.input :eis_sync_date

  .form-actions
    = f.button :wrapped,
               :delete => @service_provider,
               :cancel => service_providers_path,
               :data => { :disable_with => t(:saving, :scope => :forms) }
