= simple_form_for(@media_region, :html => { :class => 'form-horizontal' }) do |f|
  = f.error_messages

  .form-inputs
    = f.input :uid
    = f.input :code
    = f.input :code_external
    = f.input :name
    = f.input :eis_sync_date

  .form-actions
    = f.button :wrapped,
               :delete => @media_region,
               :cancel => media_regions_path,
               :data => { :disable_with => t(:saving, :scope => :forms) }
