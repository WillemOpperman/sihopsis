        .row-fluid
          .span2
            %b
              Tax nett amount
          .span10
            = @campaign_schedule_inventory_check.tax_nett_amount
        .row-fluid
          .span2
            %b
              Description
          .span10
            = @campaign_schedule_inventory_check.description
        .row-fluid
          .span2
            %b
              Eis sync date
          .span10
            = @campaign_schedule_inventory_check.eis_sync_date
        .row-fluid
          .span12
            %hr
        .row-fluid
          .span2
            %b
              = t(:created_at, :scope => :forms)
          .span10
            = time_ago_tag(@campaign_schedule_inventory_check.created_at)
        .row-fluid
          .span2
            %b
              = t(:updated_at, :scope => :forms)
          .span10
            = time_ago_tag(@campaign_schedule_inventory_check.updated_at)
